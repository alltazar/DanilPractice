package danil;

import static java.lang.Math.abs;

public class MyHashMap {
    Object[] map = new Object[8];
    Para[] pary = new Para[1];

    public void set(Object key, int value){
        int index = abs(key.hashCode()) % map.length;
        if (map[index] == null) {
            map[index] = pary;
            pary[0] = new Para(key, value);
        } else {
            map[index] = pary;
            Para[] pary2 = new Para[pary.length+1];
            for (int i = 0; i < pary.length +1; i++){
                pary2[i] = pary[i];
            }
            pary = pary2;
        }
    }

    class Map{
        Para[] pary = new Para[1];
    }

    class Para{

        Object key;
        int value;

        Para(Object key, int value){
        this.key = key;
        this.value = value;
        }

        int getValue(){
            return value;
        }
    }

    public Integer get(Object key){
        int index = abs(key.hashCode()) % map.length;
        pary = (Para[]) map[index];
        Integer in = null;
        for (int i=0; i< pary.length; i++){
            if (pary[i].key == key){
                in = pary[i].getValue();
            }
        }
        return in;
    }
}
